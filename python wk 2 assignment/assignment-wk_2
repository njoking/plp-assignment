# Question 1
my_list = []

# Question 2
my_list.append(10)
my_list.append(20)
my_list.append(30)
my_list.append(40)

# Question 3
my_list.insert(1, 15)

# Question 4
my_list.extend([50, 60, 70])

# Question 5
my_list.pop()

# Question 6
my_list.sort()

# Question 7
index_30 = my_list.index(30)
print("Index of 30:", index_30)

# Question 8
print("Final List:", my_list)
